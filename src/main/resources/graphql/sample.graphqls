type Query {
  getString: String
}

type Mutation {
  parseString(payload: String!): Payload!
}

union Payload = Response | Errors

type Response {
  result: Int!
}

type Errors {
  errors: [Error]!
}

union Error = EmptyPayload | BadPayload

type EmptyPayload implements MutationError {
  message: String!
}

type BadPayload implements MutationError {
  message: String!
  payload: String!
}

interface MutationError {
  message: String!
}
